#!/bin/sh
#
# 

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd -P )"
DIR=$(dirname $(dirname $DIR))

IFS=$'\n'
for f in $(find $DIR/notebooks -name '*.ipynb'); do
    
    echo $f
    
    out=$($DIR/_ipython/convert.py "$f")
    
    if [ ! -z "$out" ]; then
        git add "$out"
        echo converted: $f
    else
        echo exists: $f
    fi
    
done

